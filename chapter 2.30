#include<iostream>
using namespace std;
int main()
{
	double  inches, pounds;
	cout << "enter weight in pounds" << endl;
	cin >> pounds;
	cout << "enter height in inches" << endl;
	cin >> inches;
	double bmi = pounds * 703 / inches * inches;
	cout << "BMI = " << bmi << endl;
	return 0;
}
